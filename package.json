{
  "name": "sogni-360",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "npx kill-port 5180 && vite --host 0.0.0.0 --port 5180 --strictPort",
    "build": "tsc && vite build",
    "build:staging": "tsc && vite build --mode staging",
    "lint": "eslint \"src/**/*.{ts,tsx,js,jsx}\" --report-unused-disable-directives --max-warnings 0",
    "validate:useeffect": "node scripts/validate-useeffect.js",
    "deploy:production": "bash scripts/deploy-production.sh",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "server:install": "cd server && npm install",
    "server:dev": "cd server && npm run dev",
    "server:start": "cd server && npm start",
    "prepare": "npm run server:install"
  },
  "dependencies": {
    "@sogni-ai/sogni-client": "4.0.0-alpha.52",
    "framer-motion": "^12.9.2",
    "jszip": "^3.10.1",
    "mp4box": "^2.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.5",
    "react-turnstile": "^1.1.4",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^29.5.14",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.31.0",
    "@typescript-eslint/parser": "^8.31.0",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.16",
    "dotenv": "^16.5.0",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-unicorn": "^47.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "playwright": "^1.58.0",
    "postcss": "^8.4.33",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.3.2",
    "vite": "^6.2.2"
  }
}
